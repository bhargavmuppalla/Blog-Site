<%- include("./partials/header") -%>
<h1>
  <%= heading %>
</h1>
<p>
  <%= content %>
</p>

<% var posts = posts %>
<% var len = posts.length%>
<% posts.forEach(function(post){ %>
  <h2>
    <%= post.Title %>
  </h2>
  <p>
    <%= post.Post.substring(0,100)+"...." %>
    <a href="posts/<%= post._id %>" > Read More </a>
  </p>
<% }); %>

<%- include("./partials/footer") -%>
