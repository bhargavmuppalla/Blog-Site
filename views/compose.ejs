<%- include("./partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" name="postTitle">
  </div>
  <div class="form-group">
    <label for="post">Post</label>
    <textarea class="form-control" name="post" rows="5" cols="80"></textarea>
  </div>
  <button class="btn btn-primary" type="Submit" name="Submit">Publish</button>
</form>
<%- include("./partials/footer") -%>
